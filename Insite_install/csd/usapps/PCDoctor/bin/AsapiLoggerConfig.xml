<pcd:logger>
   
	<logger name="Root" priority="Error" >
		<appender type="stream" name="ConsoleErr" out="err" priority="Fatal"/>
		<appender type="file" name="AsapiLogFile" priority="Debug" dir="logs" usepid="true" out="asapi.log" overwrite="true" />
	 </logger>

	<logger name="IPC" priority="Error" />
	<logger name="SharedMemoryImp" priority="Error" />

	<logger name="Benchmarking" priority="Error" >
		<appender type="file" name="AsapiBenchLogFile" priority="Info" dir="logs" usepid="true" out="asapi_benchmarks.log" overwrite="true" />
	 </logger>

	 <logger name="clientLicense" priority="Debug" >
		<appender type="file" name="LicenseClientLogFile" priority="Debug" dir="logs" usepid="true" out="LicenseClient.log" overwrite="true" />
	</logger>

	<logger name="CheckLockOS" priority="Debug" >
		<appender type="file" name="CheckLockOSLogFile" priority="Debug" dir="logs" usepid="true" out="CheckLockOS.log" overwrite="true" />
	</logger>

	<logger name="CheckLock" priority="Debug" >
		<appender type="file" name="CheckLockLogFile" priority="Debug" dir="logs" usepid="true" out="CheckLock.log" overwrite="true" />
	</logger>

	<logger name="licenseGenerator" priority="Debug" >
		<appender type="file" name="licenseGeneratorFile" priority="Debug" dir="logs" usepid="true" out="licenseGenerator.log" overwrite="true" />
	</logger>
	
</pcd:logger>
