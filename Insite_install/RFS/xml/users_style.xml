<?xml version="1.0" encoding="UTF-8"?>
<styleRFS>
		<!-- Page -->
		<constant name="PageBackground" value="#D3D3D3" />
		<constant name="PageFontFamily" value="Arial, Helvetica, sans-serif" />
		<constant name="PageFontSize" value="small" />
		<constant name="PageFontColor" value="#000" />
		<!-- User Tables -->
		<constant name="TableBorderColor" value="gray" />
		<constant name="TableBorderShadowColor" value="gray" />
		<constant name="TableBorderHighlightColor" value="lightgray" />
		<constant name="TableRowBackgroundColor" value="#FFFFF0" />
		<constant name="TableAlternateRowBackgroundColor" value="#EEE" />
		<constant name="TableHeaderRowBackgroundColor" value="darkgray" />
		<constant name="TableEditRowBackgroundColor" value="lightblue" />
		<constant name="TableActionsRowBackgroundColor" value="#E3E3E3" />
		<constant name="ActionFontColor" value="#333" />
		<!-- Normal Buttons -->
		<constant name="ButtonBackground" value="#CCC" />
		<constant name="ButtonBorderColor" value="#AAA" />
		<constant name="ButtonFontColor" value="#000" />
		<!-- Pushed Buttons -->
		<constant name="ButtonPushedBackground" value="#ADD8E6" />
		<constant name="ButtonPushedBorderColor" value="#ADD8E6" />
		<!-- Normal Controls -->
		<constant name="ControlBackground" value="#FFFFF0" />
		<constant name="ControlBorderColor" value="#808080" />
		<constant name="ControlFontColor" value="#000" />
		<!-- Controls with Invalid Data -->
		<constant name="ControlInvalidBackground" value="#FCC" />
		<constant name="ControlInvalidBorderColor" value="#F00" />
		<!-- Asterisk for Required Fields -->
		<constant name="AsteriskFontColor" value="#F00" />
</styleRFS>